{
  "properties": [
    {
      "name": "public.name",
      "type": "java.lang.String",
      "description": "Public name of the Project"
    },
    {
      "name": "public.port",
      "type": "java.lang.Integer",
      "description": "Public port of the Project"
    },
    {
      "name": "public.url",
      "type": "java.lang.String",
      "description": "Public URL/Domain of the Project"
    },
    {
      "name": "jwt.signingKey",
      "type": "java.lang.String",
      "description": "Secret key to sign JWTs."
    },
    {
      "name": "account.verificationTime",
      "type": "java.lang.Integer",
      "description": "Time (in hours) for users to verify their account via email link."
    },
    {
      "name": "admin.email",
      "type": "java.lang.String",
      "description": "Email address of the API admin"
    },
    {
      "name": "admin.password",
      "type": "java.lang.String",
      "description": "Clear string password of the API admin"
    },
    {
      "name": "spring.mail.address",
      "type": "java.lang.String",
      "description": "Sender email address"
    },
    {
      "name": "spring.mail.smtp.auth",
      "type": "java.lang.Boolean",
      "description": "Activate SMTP authentication"
    },
    {
      "name": "spring.mail.smtp.starttls.enable",
      "type": "java.lang.Boolean",
      "description": "Activiate SMTP StartTLS"
    }
  ]
}